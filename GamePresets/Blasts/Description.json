// Данное описание актуально для осколочного (Fragment), множественного основного (Multiple_main) и множественного дочернего (Multiple_child)
{
	"name" : "[DoNotLoadInGame]", // Имя взрыва, не должно повторяться. Имя которое указано здесь можно использовать повторно, 
	// однако обьекты с именем [DoNotLoadInGame] не будут загружаться при запуске игры
    "damage": 500.0, // Урон в эпицентре взрыва
	"damageLose": 25.0, // Потеря урона (чем дальше цель, тем меньше урон)
	"radius": 14.0, // Радиус взрыва
	"fragmentName": "Multiple_child~Default", // Фрагмент гранаты (оставлять пустым если отсутствует) (создаются после взрыва) (должен быть гранатой)
	"fragsCount": 10, // Количество создаваемых фрагментов
	"explosionForce": 10.0, // Сила взрыва (влияет на Screen shake эффект и на разлёт фрагментов)
	"damageType": 7, // Тип урона (взрыв)
	"randomRotation": true, // Использовать ли случайный поворот для спрайта? true / false
	"scoreValue": 1.0 // Единицы счёта (влияет на бонусные очки) (лучше не менять и удалить поле)
}
// Для описания электро-взрыва можно использовать все те же параметры, кроме того, у него есть один дополнительный:
// "damageIncrease": 0.02, // Коэффициент, определяющий зависимость урона от количества жертв (чем больше жертв, тем больше урон)



// В .json файле можно оставлять комментарии после двойного слэша "//" в любом месте
// В данном файле представлены все основные параметры для редактирования предмета, однако
