{
	"name" : "[DoNotLoadInGame]", // Имя снаряда, не должно повторяться. Имя которое указано здесь можно использовать повторно, 
	// однако обьекты с именем [DoNotLoadInGame] не будут загружаться при запуске игры
    "previewImage": "", // Картинка с предпросмотром (названия всех возможных картинок в файле GamePresets\Images.txt)
    "movementSpeed": 200.0, // Скорость передвижения пули (не рекомендуется устанавливать выше чем 500.0)
	"damage": 75.0, // Урон (при выстреле вплотную)
	"angle": 0.4, // Максимальный угол отклонения пули от прямой траектории (в градусах)
	"damageLose": 1, // Коэффициент потери урона (чем дольше летит пуля, тем меньше урон), урон = sqrt(пройденное_расстояние) * damageLose
	"minDamage": 30, // Минимальный урон (не важно, как далеко улетит пуля, урон будет не меньше этого значения)
	"damageType": 1, // Тип урона наносимый снарядом (1 - твёрдая пуля) (Типы урона можно посмотреть в файле GamePresets\DamageTypes.txt)
	"pitchDelta": 0.07 // Возможное отклонение частоты звука (лучше просто не трогать или удалить)
}

// В .json файле можно оставлять комментарии после двойного слэша "//" в любом месте
// В данном файле представлены все основные параметры для редактирования предмета, однако
// некоторые свойства можно при желании удалять (тогда параметры будут иметь значения по умолчанию)
// Например, можно удалить поле pitchDelta, т.к. его редактирование маловероятно